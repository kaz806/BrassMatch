<h1 class="page-title">マイプロフィール</h1>

<div class="center-button">
  <%= link_to "コメント一覧に戻る", tweets_path, class: "btn btn-outline-primary" %>
  </div>

<div class="user-info">
  <% if @user.present? %>
    <% user_info = [
      "名前 : #{h(@user.name)}",
      "メールアドレス : #{h(@user.email)}",
      "プロフィール : #{h(@user.profile)}"
    ] %>

    <% user_info.each do |info| %>
      <p><%= info %></p>
    <% end %>
  <% else %>
    <p>投稿者情報が取得できませんでした</p>
  <% end %>

  <% if current_user && current_user.id == @user.id %>
    <div class="center-button">
    <%= link_to "編集する", edit_user_registration_path, class: "btn btn-outline-primary" %>
    </div>
  <% end %>
</div>

<!-- ナビゲーションバー -->
<nav class="activity-nav">
  <ul>
    <li><a href="#" data-target="user-comments">コメント一覧</a></li>
    <li><a href="#" data-target="liked-posts">いいね一覧</a></li>
    <li><a href="#" data-target="diagnosis-history">診断結果一覧</a></li>
  </ul>
</nav>

<!-- コメント一覧 -->
<div id="user-comments" class="activity-section active">
  <h3 class="subsection-title">あなたのコメント一覧</h3>

  <% if @user.tweets.any? %>
    <% @user.tweets.order(created_at: :desc).each do |t| %>
      <div class="tweet-item">
        <div class="tweet-content">
          <p><strong>投稿者:</strong> <%= t.user.name %></p>
          <p><strong>曲名:</strong> <%= t.music %></p>
          <p><strong>内容:</strong> <%= t.detail %></p>
          <p class="time"><%= t.created_at.in_time_zone('Tokyo').strftime("%Y/%m/%d %H:%M") %></p>
        </div>

        <div class="action-buttons">
          <!-- いいね表示 -->
          <div class="like-section">
            <% if user_signed_in? %>
              <%= button_to like_tweet_path(t.id), method: :post, class: "btn-like-icon", aria: { label: "この投稿にいいねする" } do %>
                💗
              <% end %>
            <% end %>
            <span class="like-count"><%= t.likes_count %></span>
          </div>

          <!-- 編集・削除 -->
          <% if current_user&.id == t.user_id %>
            <%= link_to "編集", edit_tweet_path(t.id), class: "btn btn-edit" %>
            <%= link_to "削除", tweet_path(t.id), method: :delete, data: { confirm: "本当に削除しますか？" }, class: "btn btn-delete" %>
          <% end %>
        </div>
      </div>
    <% end %>
  <% else %>
  <div class="empty-box">
    <p class="empty-message">まだコメントがありません。</p>
    <p class="empty-message">気になる投稿にコメントして、あなたの想いを届けよう！</p>
  </div>
  <% end %>
</div>


<!-- いいね一覧 -->
<div  id="liked-posts" class="activity-section">
  <h3 class="subsection-title">あなたがいいねした投稿一覧</h3>

  <% if @user.liked_tweets.any? %>
  <div class="liked-tweets">
    <% @user.liked_tweets.each do |t| %>
      <div class="tweet-card">
        <div class="tweet-body">
          <p><%= simple_format(t.detail) %></p>
        </div>
        <div class="tweet-actions">
          <span class="like-icon">💗</span>
          <span class="like-count"><%= t.likes.count %></span>
        </div>
      </div>
    <% end %>
  </div>
  <% else %>
  <div class="empty-box">
    <p class="empty-message">まだいいねしていません。気になる投稿にいいねしてみよう！</p>
  </div>
  <% end %>
</div>

